{
  "manifest_version": 2,
  "name": "Confluence Enhancer",
  "version": "1.0",
  "description": "Enhance the Load New Epoch Instructions by adding formatting throughout a page",
  "permissions": [
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": { "48": "icon.png" }
}
